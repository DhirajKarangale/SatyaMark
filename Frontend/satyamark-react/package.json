{
  "name": "satyamark-react",
  "version": "1.0.0",
  "description": "Satyamark UI/status library (wrapped from your project).",
  "main": "dist/index.cjs.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "public"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "build2": "tsup src/index.ts --format esm,cjs --dts",
    "prepare": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.0"
  }
}