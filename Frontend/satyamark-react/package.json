{
  "name": "satyamark-react",
  "version": "0.0.5",
  "description": "Official React SDK for SatyaMark â€” a lightweight UI library to embed transparent content verification marks, confidence indicators, and trust signals into React applications.",
  "license": "MIT",
  "author": "DK",
  "homepage": "https://satyamark.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/DhirajKarangale/SatyaMark.git"
  },
  "bugs": {
    "url": "https://github.com/DhirajKarangale/SatyaMark/issues"
  },
  "keywords": [
    "react",
    "verification",
    "trust",
    "fact-checking",
    "ai-detection",
    "misinformation",
    "sdk",
    "ui-library",
    "open-source",
    "satyamark"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "public"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "build2": "tsup src/index.ts --format esm,cjs --dts",
    "prepare": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.0"
  }
}
